Language = {English},
Affiliation = {Dahle, OH (Corresponding Author), Indra Navia AS, N-1383 Asker, Norway.
   Langen, Tommy; Nunavath, Vimala, Syst Univ South Eastern Norway, Dept Sci \& Ind Syst, N-3616 Kongsberg, Norway.
   Dahle, Ole Henrik, Indra Navia AS, N-1383 Asker, Norway.},
DOI = {10.3390/ijerph19010223},
Article-Number = {223},
EISSN = {1660-4601},
Keywords = {noise model; Unmanned Aircraft System Traffic Management; drones; UAVs;
   accepted noise levels; U-space architecture},
