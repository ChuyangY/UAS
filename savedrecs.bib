@article{ WOS:001093867600001,
Author = {Wang, Lin and Clayton, Michael and Rossberg, Axel G.},
Title = {Drone audition for bioacoustic monitoring},
Journal = {METHODS IN ECOLOGY AND EVOLUTION},
